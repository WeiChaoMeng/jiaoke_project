<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jiaoke.oa.dao.OaActCarMapper">

    <insert id="insertData" parameterType="com.jiaoke.oa.bean.OaActCar">
        insert into oa_act_car
            (id,title,`user`,license_plate,properties,cause,destination,start_time,end_time,cardinal_number,after,driving_number,state,create_time,promoter,url)
        values
            (#{id},#{title},#{user},#{licensePlate},#{properties},#{cause},#{destination},#{startTime},#{endTime},#{cardinalNumber},#{after},#{drivingNumber},#{state},#{createTime},#{promoter},#{url})
    </insert>

    <update id="updateStateById" parameterType="java.util.Map">
        update oa_act_car
        set state = #{state}
        where id = #{id}
    </update>
</mapper>