<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jiaoke.oa.dao.OaActAcceptanceMapper">

    <insert id="insertData" parameterType="com.jiaoke.oa.bean.OaActAcceptance">
        insert into oa_act_acceptance
            (id,title,factory,content,amount,accepter,operator,acceptance_date,create_time,promoter,url)
        values
            (#{id},#{title},#{factory},#{content},#{amount},#{accepter},#{operator},#{acceptanceDate},#{createTime},#{promoter},#{url})
    </insert>
</mapper>