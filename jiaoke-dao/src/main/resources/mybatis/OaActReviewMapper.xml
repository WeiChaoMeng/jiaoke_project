<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jiaoke.oa.dao.OaActReviewMapper">

    <insert id="insertData" parameterType="com.jiaoke.oa.bean.OaActReview">
        insert into oa_act_review
            (id,title,`name`,numbering,`relative`,amount,`number`,create_time,promoter,url)
        values
            (#{id},#{title},#{name},#{numbering},#{relative},#{amount},#{number},#{createTime},#{promoter},#{url})
    </insert>
</mapper>